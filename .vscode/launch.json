    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "javascript:html",
            "url": "http://127.0.0.1:5500/index.html"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "javascript",
            "program": "${file}"
        }
    ]
}